{
  "name": "rcomps",
  "version": "0.10.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build:app": "react-scripts build",
    "test": "react-scripts test",
    "tw": "react-scripts test --watch",
    "cov": "react-scripts test --coverage",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .eslintignore . --ext ts --ext tsx",
    "lint:fix": "yarn lint --fix",
    "eject": "react-scripts eject",
    "sb": "start-storybook -p 9009 -s public",
    "bsb": "build-storybook -c .storybook -o docs",
    "build": "rm -rf dist && rollup --config rollup.config-rlib.js",
    "dist": "yarn build && cp package.rlib.json dist/package.json && cd dist/ && yarn publish --access=public || true && cd .. && rm -rf dist/"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.21",
    "@reach/router": "^1.2.1",
    "@storybook/addon-knobs": "^5.2.5",
    "@storybook/addon-storyshots": "^5.2.5",
    "@types/jest": "24.0.19",
    "@types/node": "12.11.5",
    "@types/react": "16.9.9",
    "@types/react-dom": "16.9.2",
    "node-sass": "^4.12.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "3.2.0",
    "typescript": "^3.6.4"
  },
  "devDependencies": {
    "@cpmech/react-icons": "^2.3.0",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/addons": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@types/reach__router": "^1.2.6",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-jest": "^22.20.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "require-context.macro": "^1.2.2",
    "rollup": "^1.25.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-typescript2": "^0.24.3"
  }
}
